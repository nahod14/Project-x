    "playwright": "^1.52.0"
  },
  "devDependencies": {
    "@types/node": "^18.15.11",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.3"
  }
} 