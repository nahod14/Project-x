{
  "name": "scraper",
  "version": "1.0.0",
  "description": "Web scraper service for the Price Tracker application",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
